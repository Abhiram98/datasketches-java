<FindBugsFilter>
  <!-- All bugs in test classes -->
  <Match>
    <Class name="~.*\.*Test" />
  </Match>
  
  <Match>
    <Package name="java\.*" />
  </Match>

  <Match>
    <!-- Too many false positives to be useful -->
    <Bug pattern="SF_SWITCH_NO_DEFAULT" />
  </Match>
  
  <Match>
    <!-- False positive -->
    <Bug pattern="DB_DUPLICATE_BRANCHES" />
    <Class name="com.yahoo.sketches.theta.DirectQuickSelectSketch" />
    <Method name="setHashTableThreshold" />
  </Match>
  
  <Match>
    <!-- False positive -->
    <Bug pattern="DLS_DEAD_LOCAL_STORE" />
    <Class name="com.yahoo.sketches.theta.HeapCompactOrderedSketch" />
    <Method name="toByteArray" />
  </Match>
  
  <Match>
    <!-- False positive -->
    <Bug pattern="DLS_DEAD_LOCAL_STORE" />
    <Class name="com.yahoo.sketches.theta.HeapCompactSketch" />
    <Method name="toByteArray" />
  </Match>
  
